<?xml version="1.0" encoding="utf-8"?>
<s:Application xmlns:fx="http://ns.adobe.com/mxml/2009" 
			   xmlns:s="library://ns.adobe.com/flex/spark" 
			   xmlns:mx="library://ns.adobe.com/flex/mx" minWidth="955" minHeight="600"
			   xmlns:gmcomp="geo.model.components.*" creationComplete="init()">
	<fx:Script>
		<![CDATA[
			import assets.icons.EmbeddedIcons;
		]]>
	</fx:Script>
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	
	
	<fx:Script>
		<![CDATA[
			
			private function init():void {
				capList.wpsURL = "http://localhost:18080/geoserver/wps";
			}
			
		]]>
	</fx:Script>
	<s:HGroup>
		<s:VGroup>
			<gmcomp:Capabilities id="capList" />
			<gmcomp:Console id="con" />
		</s:VGroup>
		
		<s:Button icon="{assets.icons.EmbeddedIcons.hammer}" />
	</s:HGroup>
</s:Application>
